{
  "name": "usi",
  "description": "A universal JS static site generator for Instagram built with React.",
  "version": "1.0.0",
  "author": "Luke Karrys",
  "bugs": {
    "url": "https://github.com/lukekarrys/universal-static-instagram/issues"
  },
  "dependencies": {
    "andlog": "^1.0.0",
    "basscss": "^7.0.4",
    "history": "^1.11.1",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "normalizr": "^1.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc1",
    "rebass": "^0.1.5",
    "redux": "^3.0.2",
    "redux-logger": "^2.0.1",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "xhr": "^2.1.0",
    "zero-fill": "^2.2.1"
  },
  "devDependencies": {
    "@lukekarrys/eslint-config": "^1.0.24",
    "async": "^1.4.2",
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "colors": "^1.1.2",
    "cpr": "^0.4.3",
    "css-loader": "^0.19.0",
    "cssnano": "^3.3.1",
    "debug": "^2.2.0",
    "eslint": "^1.8.0",
    "eslint-plugin-classes": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "git-directory-deploy": "^1.3.0",
    "hjs-webpack": "^3.0.0",
    "http-server": "^0.8.5",
    "humps": "^1.0.0",
    "inquirer": "^0.10.1",
    "instagram-download": "^1.1.4",
    "instagram-node": "^0.5.8",
    "minimist": "^1.2.0",
    "on-build-webpack": "^0.1.0",
    "postcss-loader": "^0.6.0",
    "precommit-hook": "^3.0.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "style-loader": "^0.12.4",
    "surge": "^0.16.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "homepage": "https://github.com/lukekarrys/universal-static-instagram",
  "keywords": [
    "instagram",
    "isomorphic",
    "react",
    "react-router",
    "static site",
    "universal"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "lint",
    "validate"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukekarrys/universal-static-instagram.git"
  },
  "scripts": {
    "build": "npm run webpack.config && webpack --progress",
    "deploy.gh": "git-directory-deploy --directory public",
    "deploy.surge": "surge public",
    "fetch.data": "DEBUG=instagram-download:* babel-node server/data/fetch.js",
    "lint": "eslint --ignore-path .gitignore --ext .js,.es6 .",
    "make.config": "babel-node server/config/make.js",
    "start": "npm run webpack.config && webpack-dev-server --progress",
    "static": "http-server ./public -p 8080",
    "validate": "npm ls >/dev/null",
    "webpack.config": "babel webpack.config.es6 --out-file webpack.config.js"
  }
}
