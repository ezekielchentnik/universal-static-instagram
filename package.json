  "bugs": {
    "url": "https://github.com/lukekarrys/jekyll-instagram/issues"
  },
  "dependencies": {
    "alt": "^0.17.0",
    "async": "^1.3.0",
    "instagram-node": "^0.5.7",
    "iso": "^4.1.0",
    "lodash": "^3.10.0",
    "moment": "^2.10.3",
    "react": "^0.13.3",
    "react-router": "^1.0.0-beta2",
    "xhr": "^2.0.2",
    "zero-fill": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-eslint": "^3.1.20",
    "babel-loader": "^5.2.2",
    "css-loader": "^0.15.1",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.6.4",
    "file-loader": "^0.8.4",
    "git-directory-deploy": "^1.1.0",
    "hjs-webpack": "^2.9.0",
    "http-server": "^0.8.0",
    "json-loader": "^0.5.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "node-libs-browser": "^0.5.2",
    "postcss-loader": "^0.5.1",
    "precommit-hook": "^3.0.0",
    "react-hot-loader": "^1.2.7",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "homepage": "https://github.com/lukekarrys/jekyll-instagram",
  "keywords": [
    "universal",
    "isomorphic",
    "instagram",
    "react",
    "react-router",
    "static site"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "lint",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukekarrys/jekyll-instagram.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack && npm run copy-data",
    "copy-data": "cp -r _cache/media public/media",
    "deploy": "git-directory-deploy --directory public",
    "lint": "eslint --ext .js,.es6 .",
    "serve-local": "http-server ./public -p 3000",
    "start": "webpack-dev-server",
    "validate": "npm ls"
  }
}
